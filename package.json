{
  "name": "sas7bdat-web",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && mkdir build && cp src/index.html build && cp -r src/css build/css && NODE_ENV=production browserify src/app.js > build/app.js",
    "watch": "watchify -d src/app.js -o build/app.js"
  },
  "author": "Jeremy Scheff <jdscheff@gmail.com>",
  "license": "AGPL-3.0",
  "dependencies": {
    "babel-runtime": "^6.9.0",
    "classnames": "^2.2.5",
    "csv-stringify": "^1.0.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "sas7bdat": "^0.1.5"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ],
    "presets": [
      "es2015",
      "react"
    ]
  }
}
